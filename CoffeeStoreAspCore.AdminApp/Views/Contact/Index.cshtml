@model IEnumerable<CoffeeStoreAspCore.ViewModels.Catalog.Contacts.ContactViewModel>

@{
    ViewData["Title"] = "Feedback";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">



    <div class="card mb-4 mt-4 ">
        <div class="card-header"><i class="fas fa-table mr-1"></i>Feedback</div>
        <div class="card-body">
            <div class="table-responsive">

                <table class="table table-bordered wow slideInUp" id="dataTable" width="100%" cellspacing="0">
                    <thead style=" background:#ffd800 ">
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Id)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Email)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.PhoneNumber)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Message)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody class=" ">
                        @foreach (var item in Model)
                        {
                            <tr class="">
                                <td>
                                    @Html.DisplayFor(modelItem => item.Id)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Email)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Message)
                                </td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>